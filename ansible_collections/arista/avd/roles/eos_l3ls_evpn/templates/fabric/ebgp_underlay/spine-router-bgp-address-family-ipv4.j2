{# Spine - address family ipv4 activation #}
{% if type == "spine" %}
    peer_groups:
{%     if (underlay_routing_protocol|lower == "bgp" or underlay_routing_protocol|lower == "ebgp") %}
      IPv4-UNDERLAY-PEERS:
        activate: true
{%     endif %}
      EVPN-OVERLAY-PEERS:
        activate: false
{% endif %}
